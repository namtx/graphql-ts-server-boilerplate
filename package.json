{
  "name": "graphql-ts-server-boilerplate",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/expect-puppeteer": "^3.3.1",
    "@types/ioredis": "^4.0.10",
    "@types/jest": "^24.0.11",
    "@types/jest-environment-puppeteer": "^4.0.0",
    "@types/node": "^8.0.29",
    "@types/puppeteer": "^1.12.3",
    "@types/uuid": "^3.4.4",
    "@types/yup": "^0.26.12",
    "gql2ts": "^1.10.1",
    "jest": "^24.7.1",
    "jest-puppeteer": "^4.1.1",
    "nodemon": "^1.18.11",
    "puppeteer": "^1.14.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "3.3.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "2.5.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "graphql-import": "^0.7.1",
    "graphql-request": "^1.8.2",
    "graphql-yoga": "^1.17.4",
    "ioredis": "^4.9.0",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.16",
    "uuid": "^3.3.2",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts",
    "test-server": "NODE_ENV=test ts-node src/index.ts",
    "test": "NODE_ENV=test jest --forceExit --detectOpenHandles"
  },
  "jest": {
    "globalSetup": "./src/setup_test/callSetup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
